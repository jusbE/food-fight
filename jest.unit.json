{
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "testMatch": [
    "**/*.spec.ts"
  ],
  "moduleFileExtensions": [
    "ts",
    "js"
  ],
  "cache": false,
  "globals": {
    "ts-jest": {
      "tsConfig": "tsconfig.test.json"
    }
  },
  "collectCoverageFrom": [
    "src/**/{!((spec)|(index)),}.ts"
  ]
}